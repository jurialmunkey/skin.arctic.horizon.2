    <include name="List_Core">
        <param name="orientation">horizontal</param>
        <param name="controltype">list</param>
        <param name="id"></param>
        <param name="preloaditems">2</param>
        <param name="tween">quadratic</param>
        <param name="scrolltime">400</param>
        <param name="viewtype">list</param>
        <param name="offset_x">grid_x08</param>
        <param name="offset_y">grid_x08</param>
        <param name="itemlayout_w"></param>
        <param name="itemlayout_h"></param>
        <param name="item_w"></param>
        <param name="item_h"></param>
        <param name="itemlayout_include"></param>
        <param name="focusedlayout_special">false</param>
        <param name="altfocus">false</param>
        <param name="detailed">false</param>
        <param name="indicator">true</param>
        <definition>
            <control type="$PARAM[controltype]" id="$PARAM[id]">
                <nested />
                <orientation>$PARAM[orientation]</orientation>
                <preloaditems>$PARAM[preloaditems]</preloaditems>
                <scrolltime tween="$PARAM[tween]">$PARAM[scrolltime]</scrolltime>
                <itemlayout width="$PARAM[itemlayout_w]" height="$PARAM[itemlayout_h]">
                    <control type="group">
                        <left>$PARAM[offset_x]</left>
                        <right>-$PARAM[offset_x]</right>
                        <top>$PARAM[offset_y]</top>
                        <bottom>-$PARAM[offset_y]</bottom>
                        <include content="$PARAM[itemlayout_include]">
                            <param name="selected">false</param>
                            <param name="item_w">$PARAM[item_w]</param>
                            <param name="item_h">$PARAM[item_h]</param>
                            <param name="misc">$PARAM[misc]</param>
                            <param name="id">$PARAM[id]</param>
                            <param name="detailed">$PARAM[detailed]</param>
                            <param name="indicator">$PARAM[indicator]</param>
                            <param name="variation">$PARAM[variation]</param>
                            <param name="diffuse">$PARAM[diffuse]</param>
                            <param name="icon">$PARAM[icon]</param>
                            <param name="textcolor">$PARAM[textcolor]</param>
                            <param name="shadow_colordiffuse">$PARAM[shadow_colordiffuse]</param>
                        </include>
                    </control>
                </itemlayout>
                <focusedlayout width="$PARAM[itemlayout_w]" height="$PARAM[itemlayout_h]" condition="!$PARAM[focusedlayout_special]">
                    <control type="group">
                        <left>$PARAM[offset_x]</left>
                        <right>-$PARAM[offset_x]</right>
                        <top>$PARAM[offset_y]</top>
                        <bottom>-$PARAM[offset_y]</bottom>
                        <control type="group">
                            <visible>!Control.HasFocus($PARAM[id]) + !$PARAM[altfocus]</visible>
                            <include content="$PARAM[itemlayout_include]">
                                <param name="selected">false</param>
                                <param name="item_w">$PARAM[item_w]</param>
                                <param name="item_h">$PARAM[item_h]</param>
                                <param name="misc">$PARAM[misc]</param>
                                <param name="id">$PARAM[id]</param>
                                <param name="detailed">$PARAM[detailed]</param>
                                <param name="indicator">$PARAM[indicator]</param>
                                <param name="variation">$PARAM[variation]</param>
                                <param name="diffuse">$PARAM[diffuse]</param>
                                <param name="icon">$PARAM[icon]</param>
                                <param name="textcolor">$PARAM[textcolor]</param>
                                <param name="shadow_colordiffuse">$PARAM[shadow_colordiffuse]</param>
                            </include>
                        </control>
                        <control type="group">
                            <visible>Control.HasFocus($PARAM[id]) | $PARAM[altfocus]</visible>
                            <include content="$PARAM[itemlayout_include]">
                                <param name="selected">true</param>
                                <param name="item_w">$PARAM[item_w]</param>
                                <param name="item_h">$PARAM[item_h]</param>
                                <param name="misc">$PARAM[misc]</param>
                                <param name="id">$PARAM[id]</param>
                                <param name="detailed">$PARAM[detailed]</param>
                                <param name="indicator">$PARAM[indicator]</param>
                                <param name="variation">$PARAM[variation]</param>
                                <param name="diffuse">$PARAM[diffuse]</param>
                                <param name="icon">$PARAM[icon]</param>
                                <param name="textcolor">$PARAM[textcolor]</param>
                                <param name="shadow_colordiffuse">$PARAM[shadow_colordiffuse]</param>
                            </include>
                        </control>
                    </control>
                </focusedlayout>
            </control>
        </definition>
    </include>
</includes>
