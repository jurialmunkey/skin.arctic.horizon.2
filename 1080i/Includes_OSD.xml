    <include name="OSD_TempoControl">
        <param name="textcolor">dialog_fg_70</param>
        <param name="progresscolor">dialog_fg_12</param>
        <param name="nibcolor">dialog_fg_100</param>
        <param name="selected">false</param>
        <param name="istempo">true</param>
        <definition>
            <control type="group">
                <nested />
                <control type="label">
                    <font>font_statusbar_bold</font>
                    <label>$LOCALIZE[31071]</label>
                    <include content="Object_TextColor" condition="$PARAM[selected]">
                        <param name="textcolor">$VAR[ColorSelected]</param>
                    </include>
                    <textcolor>$PARAM[textcolor]</textcolor>
                    <align>left</align>
                    <height>70</height>
                    <top>0</top>
                </control>
                <include content="Object_Control" condition="!$PARAM[istempo]">
                    <param name="control">grouplist</param>
                    <left>-9</left>
                    <right>-9</right>
                    <height>70</height>
                    <bottom>0</bottom>
                    <orientation>horizontal</orientation>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[32]</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[16]</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[8]x</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[4]x</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[2]x</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">|</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[2]x</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[4]x</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[8]x</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[16]</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="width">38</param>
                        <param name="label">$NUMBER[32]</param>
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                </include>
                <include content="Object_Control" condition="$PARAM[istempo]">
                    <param name="control">grouplist</param>
                    <left>-8</left>
                    <right>-8</right>
                    <height>70</height>
                    <bottom>0</bottom>
                    <orientation>horizontal</orientation>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="label" value="0.80" />
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="label" value="0.90" />
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="label" value="1.00" />
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="label" value="1.10" />
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="label" value="1.20" />
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="label" value="1.30" />
                        <param name="textcolor">$PARAM[textcolor]</param>
