                        <param name="label" value="1.40" />
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                    <include content="OSD_PlaySpeed_Label">
                        <param name="label" value="1.50" />
                        <param name="textcolor">$PARAM[textcolor]</param>
                        <param name="selected">$PARAM[selected]</param>
                    </include>
                </include>
                <control type="image">
                    <texture colordiffuse="$PARAM[progresscolor]" border="4">progress/progress-bg.png</texture>
                    <centertop>50%</centertop>
                    <height>12</height>
                </control>
                <control type="group">
                    <left>6</left>
                    <centertop>50%</centertop>
                    <height>24</height>
                    <width>24</width>
                    <animation effect="slide" end="52" condition="$PARAM[istempo] + String.IsEqual(Player.PlaySpeed,0.90)">Conditional</animation>
                    <animation effect="slide" end="104" condition="$PARAM[istempo] + String.IsEqual(Player.PlaySpeed,1.00) | String.IsEqual(Player.PlaySpeed,0.00)">Conditional</animation>
                    <animation effect="slide" end="156" condition="$PARAM[istempo] + String.IsEqual(Player.PlaySpeed,1.10)">Conditional</animation>
                    <animation effect="slide" end="208" condition="$PARAM[istempo] + String.IsEqual(Player.PlaySpeed,1.20)">Conditional</animation>
                    <animation effect="slide" end="260" condition="$PARAM[istempo] + String.IsEqual(Player.PlaySpeed,1.30)">Conditional</animation>
                    <animation effect="slide" end="312" condition="$PARAM[istempo] + String.IsEqual(Player.PlaySpeed,1.40)">Conditional</animation>
                    <animation effect="slide" end="364" condition="$PARAM[istempo] + String.IsEqual(Player.PlaySpeed,1.50)">Conditional</animation>

                    <animation effect="slide" end="-8" condition="!$PARAM[istempo] + Player.Rewinding32x">Conditional</animation>
                    <animation effect="slide" end="30" condition="!$PARAM[istempo] + Player.Rewinding16x">Conditional</animation>
                    <animation effect="slide" end="68" condition="!$PARAM[istempo] + Player.Rewinding8x">Conditional</animation>
                    <animation effect="slide" end="106" condition="!$PARAM[istempo] + Player.Rewinding4x">Conditional</animation>
                    <animation effect="slide" end="144" condition="!$PARAM[istempo] + Player.Rewinding2x">Conditional</animation>
                    <animation effect="slide" end="182" condition="!$PARAM[istempo] + !Player.Rewinding + !Player.Forwarding">Conditional</animation>
                    <animation effect="slide" end="220" condition="!$PARAM[istempo] + Player.Forwarding2x">Conditional</animation>
                    <animation effect="slide" end="258" condition="!$PARAM[istempo] + Player.Forwarding4x">Conditional</animation>
                    <animation effect="slide" end="296" condition="!$PARAM[istempo] + Player.Forwarding8x">Conditional</animation>
                    <animation effect="slide" end="334" condition="!$PARAM[istempo] + Player.Forwarding16x">Conditional</animation>
                    <animation effect="slide" end="372" condition="!$PARAM[istempo] + Player.Forwarding32x">Conditional</animation>
                    <control type="image">
                        <include content="Object_ColorDiffuse" condition="$PARAM[selected]">
                            <param name="colordiffuse">$VAR[ColorSelected]</param>
                        </include>
                        <colordiffuse>$PARAM[nibcolor]</colordiffuse>
                        <texture>common/circle.png</texture>
                    </control>
                    <control type="image">
                        <centerleft>50%</centerleft>
                        <bottom>-12</bottom>
                        <height>16</height>
                        <width>16</width>
                        <include content="Object_ColorDiffuse" condition="$PARAM[selected]">
                            <param name="colordiffuse">$VAR[ColorSelected]</param>
                        </include>
                        <colordiffuse>$PARAM[nibcolor]</colordiffuse>
                        <texture flipy="true">arrows/arrow-menu-up.png</texture>
                    </control>
                </control>
            </control>
        </definition>
    </include>
</includes>
