<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1199">
    <zorder>10</zorder>
    <visible>true</visible>
    <visible>Skin.HasSetting(DebugInfo) | Skin.HasSetting(DebugGrid)</visible>
    <controls>
        <control type="group">
            <visible>Skin.HasSetting(DebugGrid)</visible>
            <!-- Vertical 40 px lines -->
            <control type="grouplist">
                <itemgap>39</itemgap>
                <orientation>vertical</orientation>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line</include>
                <include>Vert_Red_Line</include>
            </control>

            <!-- Horizontal 40 px lines -->
            <control type="grouplist">
                <itemgap>39</itemgap>
                <orientation>horizontal</orientation>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line</include>
                <include>Horz_Red_Line</include>
                <include>Horz_Red_Line</include>
            </control>




            <!-- Screen Padding Lines -->
            <control type="image">
                <top>80</top>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>80</bottom>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <left>80</left>
                <width>1</width>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <right>80</right>
                <width>1</width>
                <texture colordiffuse="red">common/white.png</texture>
            </control>

            <!-- Outer Border Lines -->
            <control type="image">
                <top>120</top>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>120</bottom>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>

            <!-- Inner Border Lines -->
            <control type="image">
                <top>200</top>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>200</bottom>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>

            <!-- Trough -->
            <control type="image">
                <top>60</top>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>60</bottom>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <top>140</top>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>140</bottom>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>

            <control type="image">
                <top>70</top>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>70</bottom>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <top>130</top>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>130</bottom>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>


            <!-- Center Lines -->
            <control type="image">
                <top>520</top>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>520</bottom>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <centertop>50%</centertop>
                <height>1</height>
                <texture colordiffuse="blue">common/white.png</texture>
            </control>
            <control type="image">
                <centerleft>50%</centerleft>
                <width>1</width>
                <texture colordiffuse="blue">common/white.png</texture>
            </control>

        </control>

        <control type="grouplist">
            <visible>Skin.HasSetting(DebugInfo)</visible>
            <align>top</align>
            <orientation>vertical</orientation>
            <include content="Overlay_InfoLabel">
                <label> --system.currentcontrolid: $INFO[System.CurrentControlID,, ]$INFO[System.CurrentControl] </label>
                <textcolor>yellowgreen</textcolor>
            </include>
            <include content="Overlay_InfoLabel">
                <label>    --system.currentwindow: $INFO[System.CurrentWindow] </label>
                <textcolor>yellowgreen</textcolor>
            </include>
            <include content="Overlay_InfoLabel">
                <label>       --contextmenu.title: $INFO[Window(Home).Property(ContextMenu.Title)] </label>
                <textcolor>yellowgreen</textcolor>
            </include>
            <include content="Overlay_InfoLabel">
                <label>   --tmdbhelper.isupdating: $INFO[Window(Home).Property(TMDBHelper.IsUpdating),details: , ]$INFO[Window(Home).Property(TMDBHelper.IsUpdatingRatings),ratings: , ]</label>
                <textcolor>yellowgreen</textcolor>
            </include>

        </control>
        <control type="grouplist">
            <visible>Skin.HasSetting(DebugInfo)</visible>
            <align>bottom</align>
            <orientation>vertical</orientation>
            <include content="Overlay_InfoLabel">
                <label> --container.folderpath: $INFO[Container.FolderPath] </label>
                <textcolor>magenta</textcolor>
            </include>
            <include content="Overlay_InfoLabel">
                <label>  --listitem.folderpath: $INFO[ListItem.FolderPath] </label>
                <textcolor>blueviolet</textcolor>
            </include>
        </control>
    </controls>
</window>