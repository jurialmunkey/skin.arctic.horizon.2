<?xml version="1.0" encoding="UTF-8"?>
<window>
    <controls>
        <include>Object_Overlay</include>
        <include content="Dialog_Background">
            <include>Animation_SlideIn_Dialog</include>
            <include>Dimension_DialogConfirm</include>
            <include>Dialog_Header</include>
            <control type="group">
                <height>grid_x16</height>
                <bottom>0</bottom>
                <include>Dialog_Overlay_V</include>
                <include>Object_Separator_V</include>
            </control>
            <include content="Dialog_Group">
                <control type="group">
                    <include content="Dialog_Textbox">
                        <bottom>16</bottom>
                    </include>
                    <bottom>grid_x20</bottom>
                </control>
                <control type="group">
                    <bottom>grid_x16</bottom>
                    <height>16</height>
                    <control type="progress" id="20">
                        <description>Progressbar</description>
                        <info>System.Progressbar</info>
                        <texturebg colordiffuse="dialog_fg_30" border="4">progress/progress-bg.png</texturebg>
                    </control>
                    <control type="progress">
                        <description>Progressbar</description>
                        <info>System.Progressbar</info>
                        <texturebg colordiffuse="00ffffff" border="4">progress/progress-bg.png</texturebg>
                        <lefttexture colordiffuse="00ffffff" border="4">progress/progress-left.png</lefttexture>
                        <midtexture colordiffuse="$VAR[ColorGradient]" border="4" diffuse="progress/diffuse.png">progress/progress-end.png</midtexture>
                        <righttexture colordiffuse="$VAR[ColorGradient]" border="4" diffuse="progress/diffuse-end.png">progress/progress-right.png</righttexture>
                        <include content="Defs_Progress_Colorfade">
                            <param name="id" value="20" />
                        </include>
                    </control>
                </control>
            </include>
            <include content="Dialog_Button_Grouplist_Bottom">
                <control type="button" id="12" description="Extra">
                    <width>dialog_button_w</width>
                </control>
                <control type="button" id="11" description="Yes">
                    <width>dialog_button_w</width>
                    <label>107</label>
                </control>
                <control type="button" id="10" description="No">
                    <width>dialog_button_w</width>
                    <label>106</label>
                </control>
            </include>
        </include>
    </controls>
