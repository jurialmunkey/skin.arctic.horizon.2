<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>1960-1969</name>
    <match>all</match>
    <rule field="year" operator="greaterthan">
        <value>1959</value>
    </rule>
    <rule field="year" operator="lessthan">
        <value>1970</value>
    </rule>
    <order direction="descending">year</order>
</smartplaylist>
