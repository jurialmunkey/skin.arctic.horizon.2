<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>1920-1929</name>
    <match>all</match>
    <rule field="year" operator="greaterthan">
        <value>1919</value>
    </rule>
    <rule field="year" operator="lessthan">
        <value>1930</value>
    </rule>
    <order direction="descending">year</order>
</smartplaylist>
