<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>1880-1919</name>
    <match>all</match>
    <rule field="year" operator="greaterthan">
        <value>1878</value>
    </rule>
    <rule field="year" operator="lessthan">
        <value>1920</value>
    </rule>
    <order direction="descending">year</order>
</smartplaylist>
