<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>2000-2009</name>
    <match>all</match>
    <rule field="year" operator="greaterthan">
        <value>1999</value>
    </rule>
    <rule field="year" operator="lessthan">
        <value>2010</value>
    </rule>
    <order direction="descending">year</order>
</smartplaylist>
